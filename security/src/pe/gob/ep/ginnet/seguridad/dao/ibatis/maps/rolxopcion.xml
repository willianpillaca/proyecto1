   <select id="selectTreeRolOpcion" parameterClass="java.util.HashMap" resultMap="selectTree_BaseResultMap" >
	SELECT
			RXO.I_ROLXOPCION_ID
			,RXO.I_ROLXOPCION_ROL_ID I_ROL_ID
			,O.V_OPCION_NOMBRE
			,O.I_OPCION_ID
			,O.I_OPCION_INDPADRE
			,O.V_OPCION_ACTION
			,O.I_OPCION_MODULO
			,(SELECT COUNT(*) FROM ctrl_seg_opcion OPC WHERE
			OPC.I_OPCION_INDPADRE=O.I_OPCION_ID ) AS CHILDRENS
	FROM 
		ctrl_seg_opcion O
	LEFT OUTER JOIN 
