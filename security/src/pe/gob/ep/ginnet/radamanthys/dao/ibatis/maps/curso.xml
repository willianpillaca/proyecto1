  <select id="getCursosByIdAlumno" parameterClass="java.util.HashMap" resultMap="getAllPreguntasByParameters" >      	
   	SELECT a.id , a.cod_curso, a.nombre,a.tipo_evaluacion, a.id_profesor, 
   	Concat(b.V_USUARIO_AP_PATERNO, ' ', b.V_USUARIO_AP_MATERNO,' ', b.V_USUARIO_NOMBRES) as profesor FROM tbl_curso a 
   	INNER JOIN ctrl_seg_usuario b  ON b.I_USUARIO_ID = a.id_profesor 
   
  </select>
  
